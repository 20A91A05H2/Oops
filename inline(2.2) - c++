#include<iostream>
using namespace std;
inline int cube(int a)
{
return a*a*a;
}
int main()
{
	int a;
	cout<<"Enter a value:";
	cin>>a;
	cout<<"cube of"<<a<<"is:"<<cube(a)<<endl;
	return 0;
}

Output:
Enter a value:3
cube of3is:27

--------------------------------
Process exited after 7.036 seconds with return value 0
