#include<iostream>
using namespace std;
class parent1
{
	public:
		
		parent1()
		{
			cout<<"Inside first base class constructor"<<endl;
		}
		~parent1()
		{
			cout<<"Inside first base class destructor"<<endl;
		}
};
class parent2
{
	public:
		
		parent2()
		{
				cout<<"Inside first base class constructor"<<endl;
		}
		~parent2()
		{
			cout<<"Inside first base class destructor"<<endl;
		}
};
class child : public parent1,public parent2
{
	public:
		
		child()
		{
				cout<<"Inside first base class constructor"<<endl;
		}
		~child()
		{
			cout<<"Inside first base class destructor"<<endl;
		}
};

int main()
{
	child obj1;
	return 0;
}

